# [0, 1, 1, 2, 3, 5, 8, 13...]
def fibonacci(n):
    if(n<0):
        print("enter a value greater than 0")
    else:
        if(n==0):
            return 0
        elif(n==1):
            return 0
        elif(n==2):
            return 1
        else: 
            return  fibonacci(n-1)+fibonacci(n-2)
  

print(fibonacci(8))
