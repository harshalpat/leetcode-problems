def getNewString(arr):
    for i in range(len(arr)):
        s=''
        for j in arr[i]:
            # print(j,end='')
            s += s.join(chr(ord(j)+1))
        print(str(s))
        t.append(s)
    print(t)
        
t=[]
          
getNewString(['har','pat','nicks'])
